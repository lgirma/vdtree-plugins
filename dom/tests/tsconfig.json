{
    "extends": "../tsconfig.json",
    "include": [
        "tests/**/*"
    ],
    "exclude": [
        "node_modules/*",
        "__sapper__/*",
        "dist/*"
    ],
    "compilerOptions": {
        "esModuleInterop": true,
        "moduleResolution": "node",
        "module": "commonjs",
        "strict": false,
        "noEmit": false,
        "target": "ES2015",
        "downlevelIteration": true
    }
}